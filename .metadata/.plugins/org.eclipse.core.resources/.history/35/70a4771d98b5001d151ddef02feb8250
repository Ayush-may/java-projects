
public class Paddle {
	
	private int paddleDimX;
	private int paddleDimY;
	private int paddleX ;
	private int paddleY ;
	
	Paddle(int paddleDimX,int paddleDimY,int paddleX, int paddleY){
		this.paddleX = paddleX;
		this.paddleY = paddleY;
		this.paddleDimX = paddleDimX;
		this.paddleDimY = paddleDimY;
	}
	
	public int getPaddleDimX() {
		return paddleDimX;
	}
	
	public int getPaddleDimY() {
		return paddleDimY;
	}
	
	public int getX() {
		return paddleX;
	}
	
	public int getY() {
		return paddleY;
	}	
}
